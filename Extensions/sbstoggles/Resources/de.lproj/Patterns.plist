<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "--//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">

<plist version="1.0">
	<dict>
		<key>example</key>
		<string>SBSettings Toggles</string>
		<key>icon</key>
		<string>Icon</string>
		<key>patterns</key>
		<dict>
			<key>TurnOn</key>
			<dict>
				<key>category</key>
				<string>Ein Toggle einschalten</string>
				<key>examples</key>
				<array>
					<string>Wi-Fi einschalten</string>
					<string>Wi-Fi an</string>
					<string>Wi-Fi anmachen</string>
				</array>
				<key>patterns</key>
				<array>
					<string>(t:&lt;word&gt;+) einschalten</string>
					<string>(t:&lt;word&gt;+) an</string>
					<string>(t:&lt;word&gt;+) anmachen</string>
				</array>
			</dict>
			
			<key>TurnOff</key>
			<dict>
				<key>category</key>
				<string>Turning off a Toggle</string>
				<key>examples</key>
				<array>
					<string>Wi-Fi aussschalten</string>
					<string>Wi-Fi aus</string>
					<string>Wi-Fi ausmachen</string>
				</array>
				<key>patterns</key>
                <array>
                    <string>(t:&lt;word&gt;+) auschalten</string>
                    <string>(t:&lt;word&gt;+) aus</string>
                    <string>(t:&lt;word&gt;+) ausmachen</string>
                </array>
			</dict>

			<key>List</key>
            <dict>
                <key>category</key>
                <string>Listing Toggles</string>
                <key>examples</key>
                <array>
                    <string>Toggles anzeigen</string>
                    <string>Einstellungsmöglichkeiten anzeigen</string>
                </array>
                <key>patterns</key>
                <array>
                    <string>toggles anzeigen</string>
                    <string>einstellungsmöglichkeiten anzeigen</string>
                </array>
            </dict>

		</dict>
	</dict>
</plist>
